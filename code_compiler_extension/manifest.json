{
    "manifest_version": 3,
    "name": "Online Compiler",
    "description": "Run code in Python, Java, and C++ directly from the browser.",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "host_permissions": [
      "http://localhost:5000/*"
    ]
  }
  